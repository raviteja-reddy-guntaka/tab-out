{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "resources/images/appIcon/favicon-16X16.png"
  },
  "icons": {
    "16": "resources/images/appIcon/favicon-16X16.png",
    "32": "resources/images/appIcon/favicon-32x32.png",
    "48": "resources/images/appIcon/android-chrome-192x192.png",
    "128": "resources/images/appIcon/android-chrome-192x192.png"
  }
}